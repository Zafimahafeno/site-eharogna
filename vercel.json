{
    "version": 2,
    "builds": [
      {
        "src": "index.ts", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "index.ts"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "index.ts"
      }
    ]
  }
  
